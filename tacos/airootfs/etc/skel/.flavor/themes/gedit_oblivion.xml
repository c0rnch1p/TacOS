<style-scheme name="Obliviongud" id="obliviongud" version="1.0">
  <author>Paolo Borelli</author>
  <description>Dark color scheme using the Tango color palette</description>

  <style name="text" foreground="#d3d7cf" background="#0d0d0d" />
  <style name="selection" foreground="#eeeeec" background="#888a85" />
  <style name="cursor" foreground="#d3d7cf" />
  <style name="secondary-cursor" foreground="#a1a49e" />
  <style name="current-line" background="#555753" />
  <style name="line-numbers" foreground="#9c9c9c" background="#000000" />
  <style name="draw-spaces" foreground="#888a85" />
  <style name="background-pattern" background="#34393A" />
  <style name="bracket-match" foreground="#c17d11" />
  <style name="bracket-mismatch" foreground="#eeeeec" background="#cc0000" />
  <style name="right-margin" foreground="#eeeeec" background="#babdb6" />
  <style name="search-match" foreground="#eeeeec" background="#4e9a06" />
  <style name="def:comment" foreground="#888a85" />
  <style name="def:shebang" foreground="#888a85" bold="true" />
  <style name="def:doc-comment-element" italic="true" />
  <style name="def:constant" foreground="#edd400" />
  <style name="def:string" foreground="#edd400" />
  <style name="def:special-char" foreground="#ce5c00" />
  <style name="def:special-constant" foreground="#ce5c00" />
  <style name="def:floating-point" foreground="#ce5c00" />
  <style name="def:identifier" foreground="#729fcf" />
  <style name="def:statement" foreground="#ffffff" bold="true" />
  <style name="def:type" foreground="#8ae234" bold="true" />
  <style name="def:emphasis" italic="true" />
  <style name="def:strong-emphasis" foreground="#ffffff" bold="true" />
  <style name="def:inline-code" foreground="#729fcf" />
  <style name="def:insertion" underline="true" />
  <style name="def:deletion" strikethrough="true" />
  <style name="def:link-text" foreground="#888a85" />
  <style name="def:link-symbol" foreground="#888a85" bold="true" />
  <style name="def:link-destination" italic="true" underline="true" />
  <style name="def:heading" foreground="#8ae234" bold="true" />
  <style name="def:thematic-break" foreground="#8ae234" bold="true" />
  <style name="def:preformatted-section" foreground="#729fcf" />
  <style name="def:list-marker" foreground="#ffffff" bold="true" />
  <style name="def:preprocessor" foreground="#ad7fa8" />
  <style name="def:error" foreground="#eeeeec" background="#cc0000" bold="true" />
  <style name="def:warning" foreground="#eeeeec" background="#ad7fa8" />
  <style name="def:note" foreground="#888a85" background="#fce94f" bold="true" />
  <style name="def:net-address" italic="true" underline="true" />
  <style name="diff:added-line" foreground="#edd400" />
  <style name="diff:removed-line" foreground="#729fcf" />
  <style name="diff:changed-line" foreground="#ad7fa8" />
  <style name="diff:diff-file" foreground="#8ae234" bold="true" />
  <style name="diff:location" foreground="#8ae234" />
  <style name="diff:special-case" foreground="#ffffff" bold="true" />
  <style name="latex:command" foreground="#8ae234" bold="true" />
  <style name="latex:include" foreground="#ad7fa8" />
</style-scheme>
