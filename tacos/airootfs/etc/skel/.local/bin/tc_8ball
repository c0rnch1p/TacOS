#!/bin/bash
body(){
	local AFFIRMATIVE NONCOMMITTAL NEGATORY ANSWERS LEN INDEX
	AFFIRMATIVE=(
		"It is certain"
		"Decidedly so"
		"Without a doubt"
		"Yes, definitely"
		"You may rely on it"
		"As it is seen, yes"
		"Most likely"
		"Outlook is good"
		"Yes"
		"Signs point to yes"
	)
